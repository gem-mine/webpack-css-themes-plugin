{
  "type": "object",
  "additionalProperties": false,
  "required": ["themes"],
  "properties": {
    "themes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["entryPath"],
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "entryPath": {
            "type": "string",
            "minLength": 1
          },
          "distFileName": {
            "type": "string",
            "minLength": 1
          },
          "distChunkFilename": {
            "type": "string"
          }
        }
      }
    },
    "pre-processor": {
      "type": "string",
      "enum": ["less", "sass"]
    },
    "publicPath": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "instanceof": "Function"
        }
      ]
    }
  }
}